@{
}
<h1>Schooling Activity Form</h1>

<form asp-action="Add" asp-controller="Schooling" method="post">
    <div>
        <label asp-for="StartTime">Start Time:</label>
        <input asp-for="StartTime" />
    </div>
    <div>
        <label asp-for="UserInfo.FirstName">First Name:</label>
        <input asp-for="UserInfo.FirstName" />
    </div>
    <div>
        <label asp-for="UserInfo.LastName">Last Name:</label>
        <input asp-for="UserInfo.LastName" />
    </div>
    <div>
        <label asp-for="UserInfo.Age">Age:</label>
        <input type="number" asp-for="UserInfo.Age" />
    </div>
    <div>
        <label asp-for="UserInfo.Email">Email:</label>
        <input type="email" asp-for="UserInfo.Email" />
    </div>
    <div>
        <label asp-for="AchievedEtape">Achieved Etape:</label>
        <input asp-for="AchievedEtape" />
    </div>
    <button type="submit">Submit</button>
</form>